# Configuration du serveur
PORT=3000
NODE_ENV=development

# Configuration de la base de données
DATABASE_URL=postgresql://user:password@localhost:5432/database_name

# Configuration JWT
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=31536000

# Configuration Supabase (optionnel)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Configuration Gemini API (pour la reconnaissance d'image)
# Obtenez votre clé API depuis: https://aistudio.google.com/app/apikey
GEMINI_KEY=votre_cle_api_gemini_ici
# Modèle Gemini à utiliser (optionnel, défaut: gemini-2.0-flash)
# Options: gemini-2.0-flash, gemini-2.5-flash, gemini-2.5-pro
# GEMINI_MODEL=gemini-2.0-flash

